{
  // Use  to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
      {
          "type": "java",
          "name": "Debug (Launch)",
          "request": "launch",
          "mainClass": ""
      },
      {
          "type": "java",
          "name": "Debug (Attach)",
          "request": "attach",
          "hostName": "localhost",
          "port": "<debug port of the debuggee>"
      },
      {
          "type": "node",
          "request": "attach",
          "name": "Docker: Attach to Node",
          "remoteRoot": "/usr/src/app"
      },
      {
          "name": "Docker .NET Core Attach (Preview)",
          "type": "docker",
          "request": "attach",
          "platform": "netCore",
          "sourceFileMap": {
              "/src": "${workspaceFolder}"
          }
      }
  ]
}
